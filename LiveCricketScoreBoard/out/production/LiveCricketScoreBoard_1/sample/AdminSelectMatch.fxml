<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminSelectMatchController">
   <children>
      <Label layoutX="56.0" layoutY="201.0" prefHeight="36.0" prefWidth="193.0" text="Enter Match Code:" textFill="#cd1010">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="matchCode_value" layoutX="262.0" layoutY="201.0" prefHeight="36.0" prefWidth="176.0" />
      <Button fx:id="matchCode" layoutX="287.0" layoutY="134.0" mnemonicParsing="false" onAction="#matchCodeAction" prefHeight="45.0" prefWidth="152.0" text="See Match Code" textFill="#62c38c">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="enter" layoutX="370.0" layoutY="255.0" mnemonicParsing="false" onAction="#enterAction" prefHeight="36.0" prefWidth="68.0" text="Enter" textFill="#62c38c">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Label layoutX="202.0" layoutY="28.0" prefHeight="57.0" prefWidth="322.0" text="Select Match To Update" textFill="#23701d" textOverrun="CLIP">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
